generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User{
  id          String            @id     @default(uuid())
  phone       String            @unique
  password    String
  firstName   String
  middleName  String?
  lastName    String
  createdAt   DateTime          @default(now())
  updatedAt   DateTime          @updatedAt
}

